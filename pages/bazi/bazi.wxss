.wrap {
  width: 92%;
  padding: 0 4%;
}

.result {
  float: left;
  width: 100%;
  color: #333;
  border: 1rpx solid #999;
  font-size: 24rpx;
  margin-bottom: 30rpx;
  margin-top: 15rpx;
}
.eight {
  float: left;
  width: 100%;
}

.eight-title {
  float: left;
  width: 100%;
  height: 64rpx;
  line-height: 64rpx;
  border-bottom: 1rpx solid #999;
  text-indent: 25rpx;
  color: #47ad9f;
}

.eight-array {
  float: left;
  width: 100%;
  border-bottom: 1rpx solid #999;
}

.eight-array_content {
  float: left;
  width: 12.5%;
  height: 140rpx;
  padding: 30rpx 28rpx 15rpx 45rpx;
  box-sizing: border-box;
  border-right: 1rpx solid #999;
  position: relative;
}

.eight-array_content_border {
  border-right: none;
}

.eight-array_content view:nth-child(2) {
  color: #14ab09;
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  width: 24rpx;
}




